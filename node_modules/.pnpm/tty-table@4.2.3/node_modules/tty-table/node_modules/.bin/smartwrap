#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules/smartwrap/src/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules/smartwrap/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules/smartwrap/src/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules/smartwrap/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/smartwrap@2.0.2/node_modules:/home/runner/work/cli/cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../smartwrap@2.0.2/node_modules/smartwrap/src/terminal-adapter.js" "$@"
else
  exec node  "$basedir/../../../../../smartwrap@2.0.2/node_modules/smartwrap/src/terminal-adapter.js" "$@"
fi
